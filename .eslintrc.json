{
  "extends": ["airbnb", "airbnb/hooks"],
  "rules": {
    "no-unused-vars": [2, {"vars": "all", "args": "after-used"}],
    "no-console": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/button-has-type": "off",
    "quotes": ["error", "double"],
    "linebreak-style": ["error","windows"],
    "react/self-closing-comp": "off"
    // "printWidth": 120
  }
}
